{
    "compilerOptions": {
      "target": "es6",          
      "module": "commonjs",     
      "strict": true,           
      "outDir": "./dist",       
      "esModuleInterop": true,  
      "skipLibCheck": true      
    },
    "include": [
      "./**/*.ts"  // Include all TypeScript files in the FRONTEND folder and subfolders
    ],
    "exclude": [
      "node_modules"  // Exclude node_modules folder from being compiled
    ]
  }
  